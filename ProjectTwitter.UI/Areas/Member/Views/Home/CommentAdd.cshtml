                            <li><a href="newsfeed-messages.html" title="Alexis Clark"><img src="images\users\user-5.jpeg" alt="user" class="img-responsive profile-photo"><span class="online-dot"></span></a></li>
                            <li><a href="newsfeed-messages.html" title="James Carter"><img src="images\users\user-6.jpeg" alt="user" class="img-responsive profile-photo"><span class="online-dot"></span></a></li>
                            <li><a href="newsfeed-messages.html" title="Robert Cook"><img src="images\users\user-7.jpeg" alt="user" class="img-responsive profile-photo"><span class="online-dot"></span></a></li>
                            <li><a href="newsfeed-messages.html" title="Richard Bell"><img src="images\users\user-8.jpeg" alt="user" class="img-responsive profile-photo"><span class="online-dot"></span></a></li>
                            <li><a href="newsfeed-messages.html" title="Anna Young"><img src="images\users\user-9.jpeg" alt="user" class="img-responsive profile-photo"><span class="online-dot"></span></a></li>
                            <li><a href="newsfeed-messages.html" title="Julia Cox"><img src="images\users\user-10.jpeg" alt="user" class="img-responsive profile-photo"><span class="online-dot"></span></a></li>
                        </ul>
                    </div><!--chat block ends-->
                </div>
                <div class="col-md-7">
                    <div class="post-content">
                        <img src="images\post-images\1.jpeg" alt="post-image" class="img-responsive post-image">
                        <div class="post-container">
                            <img src="images\users\user-5.jpeg" alt="user" class="profile-photo-md pull-left">
                            <div class="post-detail">
                                <div class="user-info">
                                    <h5><a href="timeline.html" class="profile-link" name="TweetUserName"></a>@Model.TweetUserName <span class="following">following</span></h5>
                                </div>
                                <div class="line-divider"></div>
                                <div></div>
                                <div class="post-text">
                                    <textarea readonly name="TweetContent" class=" chat-message">@Model.TweetContent</textarea>

                                </div>
                                <div class="line-divider"></div>
                                <form action="/Member/Home/CommentAdd" method="post" enctype="multipart/form-data" role="form">
                                    <div class="post-comment">
                                        <img src="images\users\user-11.jpeg" alt="" class="profile-photo-sm">
                                        <p><a href="timeline.html" class="profile-link"></a></p>
                                        <input class="post-comment" type="text" name="CommentContent" placeholder="write comment..." />
                                        <input class="post-comment" type="hidden" name="TweetID" placeholder="write comment..." value="@Model.TweetID" />
                                        <button type="submit" class="btn-primary">Post Comment</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="col-md-2 static">
                    <div class="suggestions" id="sticky-sidebar">
                        <h4 class="grey">Who to Follow</h4>
